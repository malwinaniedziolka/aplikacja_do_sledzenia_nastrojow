<html>
  <head>
    <title><%= headTitle %></title>
    <link rel="stylesheet" href="/css/main.css" />
  </head>
  <body>
    <main>
      <%- include('partials/navigation', { menuLinks: menuLinks, activeLinkPath:
      activeLinkPath }); %>
      <h1 class="heading">Historia wpisów</h1>
      <% if (entries?.length > 0) { %>
      <ul>
        <% entries.forEach(entry => { %>
        <li>
            <h3 class="entry_title">Nastrój: <%= entry.mood %></h3>
            <p class="entry_description">Opis: <%= entry.description %></p>
            <p class="entry_rating">Opis: <%= entry.rating %></p>
            <p class="entry_date">Data: <%= entry.date %></p>
        </li>
        <% }); %>
      </ul>
      <% } else { %>
      <br />
      <div>No entries yet.</div>
      <% }%>
    </main>
  </body>
</html>